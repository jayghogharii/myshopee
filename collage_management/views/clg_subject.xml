<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="clg_subject_tree_view" model="ir.ui.view">
        <field name="name">clg.subject.tree.view</field>
        <field name="model">clg.subject</field>
        <field name="arch" type="xml">
            <tree>
                <field name="subject" />
            </tree>
        </field>
    </record>
    <record id="clg_subject_form_view" model="ir.ui.view">
        <field name="name">clg.subject.form.view</field>
        <field name="model">clg.subject</field>
        <field name="arch" type="xml">
            <form>
                <h1>Collage Subjects</h1>
                <group>
                    <field name="subject">subject</field>
                </group>
            </form>
        </field>
    </record>
</odoo>
