<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="clg_add_tree_view" model="ir.ui.view">
        <field name="name">clg.add.tree.view</field>
        <field name="model">clg.add</field>
        <field name="arch" type="xml">
            <tree>
                <field name="collagename" />
                <field name="address" />
                <field name='email'/> 
                <field name="conno"/>    
            </tree>
        </field>
    </record>
    <record id="clg_add_form_view" model="ir.ui.view">
        <field name="name">clg.add.form.view</field>
        <field name="model">clg.add</field>
        <field name="arch" type="xml">
            <form>
                <h1>Collage details</h1>
                <group>
                    <field name="collagename">Collagename</field>
                    <field name="address">Address</field>
                    <field name="email">Email</field>
                    <field name="conno">Contact No</field>
                </group>
            </form>
        </field>
    </record>
    
     

</odoo>
